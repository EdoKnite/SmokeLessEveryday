<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/progressFragment">

    <fragment
        android:id="@+id/healthFragment"
        android:name="com.example.smokelesseveryday.fragments.HealthFragment"
        android:label="HealthFragment"
        tools:layout="@layout/fragment_health"/>
    <fragment
        android:id="@+id/progressFragment"
        android:name="com.example.smokelesseveryday.fragments.ProgressFragment"
        android:label="ProgressFragment"
        tools:layout="@layout/fragment_progress">
        <action
            android:id="@+id/action_progressFragment_to_healthFragment"
            app:destination="@id/healthFragment" />
    </fragment>
</navigation>